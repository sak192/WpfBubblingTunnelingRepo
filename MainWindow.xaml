<Window x:Class="WpfApp_BubblingAndTunnelingEvents.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp_BubblingAndTunnelingEvents"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <!--Bubbling Events-->
    <StackPanel>
    <StackPanel MouseUp="StackPanel_MouseUp">
        <TextBox Width="100" Height="50" Text="{Binding Result}" Margin="10,10,0,0"></TextBox>
        <Button Width="100" Height="50"  Content="Btn1" Margin="10,10,0,0" MouseUp="Button_MouseUp"></Button>
    </StackPanel>
    <!--Bubbling Events-->
    
    <!--Tunneling Event-->
    <StackPanel PreviewMouseUp="StackPanel_MouseUp_1">
            <TextBox Width="100" Height="50" Text="{Binding Result}" Margin="10,10,0,0"></TextBox>
            <Button Width="100" Height="50"  Margin="10,10,0,0" Content="Btn2" PreviewMouseUp="Button_MouseUp_1"></Button>
    </StackPanel>
    </StackPanel>
    <!--Tunneling Event-->
</Window>
